{
  "tsdb": {
    "destination": "/usr/bin/tsdb",
    "source": "${opentsdb.package}/usr/bin/tsdb",
    "priority": 10,
    "isDirectory": false
  },
  "opentsdb-conf": {
    "destination": "/etc/opentsdb/conf",
    "source": "${opentsdb.package}/usr/share/opentsdb/etc/opentsdb",
    "priority": 10,
    "isDirectory": true
  }    
}