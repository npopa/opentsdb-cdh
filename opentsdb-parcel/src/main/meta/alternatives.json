{
  "gremlin-server": {
    "destination": "/usr/bin/gremlin-server",
    "source": "${opentsdb.package}/bin/gremlin-server.sh",
    "priority": 10,
    "isDirectory": false
  },
  "gremlin-console": {
    "destination": "/usr/bin/gremlin-console",
    "source": "${opentsdb.package}/bin/gremlin.sh",
    "priority": 10,
    "isDirectory": false
  },
  "gremlin": {
    "destination": "/usr/bin/tsdb",
    "source": "${opentsdb.package}/usr/bin/tsdb",
    "priority": 10,
    "isDirectory": false
  },
  "opentsdb-conf": {
    "destination": "/etc/opentsdb",
    "source": "${opentsdb.package}/share/opentsdb/etc/opentsdb",
    "priority": 10,
    "isDirectory": true
  }    
}